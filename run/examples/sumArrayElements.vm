program SumArrayElements;

{
  int i;
}

int sumArrElem(int[10] arr) {
  int temp;
  temp = 0;
  for(i = 0; i < 10; i = i + 1) {
    temp = temp + arr[i];
  }

  return temp;
}

void main() {
  int[10] arr;
  arr[0] = 5;
  arr[1] = 2;
  arr[2] = 3;
  arr[3] = 10;
  arr[4] = 7;
  arr[5] = 1;
  arr[6] = 4;
  arr[7] = 8;
  arr[8] = 6;
  arr[9] = 9;
  int result;
  result = sumArrElem(arr);
  print(result); // result = 55
}